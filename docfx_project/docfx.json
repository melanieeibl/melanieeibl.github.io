{
  "build": {
    "content": [
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "about/**.md",
          "about/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "Melanie Eibl",
      "_appLogoPath": "/images/favicon/icons8-camper-50.png",
      "_appFaviconPath": "/images/favicon/icons8-camper-16.png"
    },
    "fileMetadataFiles": [],
    "template": [
      "templates/default",
      "templates/darkfx",
      "templates/darkfx-mee"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}